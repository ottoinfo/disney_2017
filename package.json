{
  "name": "disney",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --display-error-details",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "clean": "rm -rf ./dist/*",
    "dev": "webpack-dev-server --open --progress --colors --inline --hot",
    "lint": "./node_modules/.bin/eslint src server",
    "start": "webpack-dev-server --open --hot",
    "test:run": "NODE_ENV=test node --harmony_proxies ./node_modules/.bin/jest",
    "test": "rm -rf test-results && npm run test:run -- --coverage",
    "test:dev": "npm run test:run -- --watch",
    "watch": "webpack --watch"
  },
  "author": "Matthew Otto <matt@ottoinfo.com> (http://ottoinfo.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "empty": "^0.10.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.1",
    "jasmine-reporters": "^2.2.1",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.6.0",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^3.1.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "es6-promise": "^4.1.1",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.3",
    "normalizr": "^3.2.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "tests"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "_api/(.*)": "<rootDir>/src/api/$1",
      "_components/(.*)": "<rootDir>/src/components/$1",
      "_containers/(.*)": "<rootDir>/src/containers/$1",
      "_layouts/(.*)": "<rootDir>/src/layouts/$1",
      "_redux/(.*)": "<rootDir>/src/redux/$1",
      "_views/(.*)": "<rootDir>/src/views/$1"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/tests/setup-jasmine-env.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup-jasmine-env.js",
    "globals": {
      "__DEV__": true
    }
  }
}
